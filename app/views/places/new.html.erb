<h1>New Place</h1>
<% if @current_user %>
  <%= form_with :model => @place do |form| %>
    <div class="mb-3">
      <%= form.label "name" %>
      <%= form.text_field "name" %>
    </div>
    <%= form.submit "New place", :class => "btn btn-primary" %>
  <% end %>
<% else %>
  You must first <a href="/login">login</a> to post
<% end %>