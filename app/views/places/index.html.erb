  <div class="row">
    <div class="col-sm-11 text-end">
      <a href="/places/new" class="btn btn-success">New place </a>
    </div>
    
    <div class="col-sm-4">
      <h1>Places</h1>
    </div>
  </div>

<ul>
  <% if @current_user %>
    <% for place in @places %>
      <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
    <% end %>
  </ul>

  <% end %>